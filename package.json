{
  "name": "detochko-ts-utils",
  "version": "1.0.0",
  "description": "TypeScript common code library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clear": "rimraf ./dist",
    "build": "npm run clear && tsc",
    "lint": "tslint -c tslint.json ./src/**/*.ts ./tests/**/*.ts",
    "tape": "ts-node --no-cache ./node_modules/tape/bin/tape ./tests/**/*.test.ts | tap-dot",
    "test": "npm run tape"
  },
  "author": "RayMan <detochko@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/detochko/ts-utils.git"
  },
  "devDependencies": {
    "@types/lodash": "4.14.74",
    "@types/tape": "4.2.30",
    "detochko-my-own-tslint": "1.0.2",
    "lodash": "4.17.4",
    "rimraf": "2.6.2",
    "tap-dot": "1.0.5",
    "tape": "4.8.0",
    "ts-node": "3.3.0",
    "tslib": "1.7.1",
    "tslint": "5.7.0",
    "typescript": "2.5.2"
  }
}
